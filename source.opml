						<outline text="else {">
							<outline text="var htmltext = &quot;&quot;, indentlevel = 0, daypath = utils.getDatePath (new Date (day.created), false), relpath = daypath + &quot;.html&quot;, path = blogConfig.basePath + relpath;"/>
							<outline text="var urlpage = blogConfig.baseUrl + relpath;"/>
